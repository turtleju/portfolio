            <div class="text-left collapsed experience-title" role="button" data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
              <%= image_tag 'chronology/older.png', class: 'chronology-icon' %>
              Graphic Design
            </div>
          </h2>
        </div>
        <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordionExperience">
